{
  "name": "cti-contact-tracing-lambda",
  "version": "1.0.0",
  "private": true,
  "description": "Lambda functions for CTI contact tracing",
  "scripts": {
    "artifact:create": "zip -r lambda.zip *",
    "cso": "env-cmd -f .env node cso.js \"{ \\\"date\\\": \\\"\\\" }\"",
    "stats": "env-cmd -f .env node stats.js"
  },
  "dependencies": {
    "@nearform/sql": "^1.4.0",
    "aws-sdk": "^2.646.0",
    "axios": "^0.19.2",
    "node-fetch": "^2.6.1",
    "openpgp": "^4.10.2",
    "pg": "^7.18.2",
    "pg-range": "^1.1.0",
    "ssh2-sftp-client": "^5.1.2",
    "twilio": "^3.47.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": "prettier-config-standard"
}
